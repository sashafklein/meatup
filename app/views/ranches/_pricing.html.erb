<% if t == "Cow" %>
	<% @ranches.each do |ranch| %>
		<% if ranch.cow %>
			<tr>
				<td><%= ranch.name %></td>
				<td><%= ranch.cow_live.round(2) if ranch.cow_live %></td>
				<td><%= ranch.cow_hanging.round(2) if ranch.cow_hanging %></td>
				<td><%= ranch.cow_meat.round(2) if ranch.cow_meat %></td>
			</tr>
		<% end %>
	<% end %>
