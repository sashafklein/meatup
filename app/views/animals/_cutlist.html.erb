<% make_list(@animal).sort.each do |p| %>
  <tr>
  	<td><%= link_to p.cut.name, p.cut %></td>
  	<td><%= number_to_currency(p.price) %></td>
	<td><%= @animal.cut_packages(p.cut).length %></td>  	
	<td><%= @animal.cut_unsold(p.cut).length %></td>
	<td><%= @animal.sold(p.cut).length %></td>
	<td><%= p.expected_weight %> lb</td>
	<td><%= (@animal.sold(p.cut).length * p.expected_weight).round(2) %></td>
  </tr>
<%end%>